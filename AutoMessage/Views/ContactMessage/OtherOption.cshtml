@model AutoMessage.ViewModel.ContactsAndMessages

<body>
    <script>
        function allowDrop(ev) { ev.preventDefault(); }
        function drag(ev) { ev.dataTransfer.setData("text/html", ev.target.id); }
        function drop(ev) {
            ev.preventDefault();
            var data = ev.dataTransfer.getData("text/html");
            ev.target.appendChild(document.getElementById(data));
        }
    </script>

    @foreach (var item in Model.contacts)
    {
        <p>@item.Name</p>
        <div id="div1" style="width:350px;height:100px;padding:10px;border:1px solid #aaaaaa;"
             ondrop="drop(event)" ondragover="allowDrop(event)"></div>
        <br>
    }

    @foreach (var item in Model.messages) {
    <p id="drag{@item.MessageId}" draggable="true" ondragstart="drag(event)">@item.Text</p>
       }

</body>  